{
  "name": "vanillajs-hello",
  "version": "1.0.0",
  "description": "Professional Boilerplate for Vanilla.js Web Apps",
  "main": "index.js",
  "scripts": {
    "c9": "webpack-dev-server --config webpack.config.js --host $IP --port $PORT",
    "gitpod": "webpack-dev-server --config webpack.config.js --host 0.0.0.0 --port 3000",
    "start": "webpack-dev-server --mode development --hot",
    "build": "webpack --mode development --verbose --colors --display-error-details",
    "deploy": "node deploy-to-github.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/4GeeksAcademy/vanillajs-hello.git"
  },
  "keywords": [
    "Vanilla.js",
    "Boilerplate",
    "WebPack",
    "Boilerplate",
    "BreatheCode"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "author": "Alejandro Sanchez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/4GeeksAcademy/vanillajs-hello/issues"
  },
  "homepage": "https://github.com/4GeeksAcademy/vanillajs-hello#readme",
  "dependencies": {
    "bootstrap": "^4.1.2",
    "breathecode-dom": "^1.0.0",
    "jquery": "^3.4.0",
    "popper.js": "^1.14.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "bc-console": "0.0.2",
    "bc-webpack-error-reporting-plugin": "0.0.1",
    "css-loader": "^2.1.1",
    "error-overlay-webpack-plugin": "^0.2.0",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "file-loader": "^1.1.5",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "gh-pages": "^1.2.0",
    "node-sass": "^4.11.0",
    "parse-github-url": "^1.0.2",
    "prettier": "^1.14.3",
    "prettier-webpack-plugin": "^1.1.0",
    "remote-origin-url": "^1.0.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  }
}
